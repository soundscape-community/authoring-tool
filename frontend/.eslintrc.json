{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app", "prettier", "eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier", "jsx-a11y"],
  "rules": { "react/prop-types": "off", "prettier/prettier": "warn", "no-unused-vars": "warn" }
}
